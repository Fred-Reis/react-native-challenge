{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {    
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "baseUrl": ".",
      "paths": {
        "*": [
          "./app/*"
        ],
        "@app/*": [
          "./app/*"
        ],
        "@app/screens/*": [
          "./screens/*"
        ],
        "@app/components/*": [
          ".app/components/*"
        ],
      }
    }
}
